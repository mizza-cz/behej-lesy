<section class="comparisonSection">
  <div class="container">
    <h3 class="text-lg titleDecore">Srovnání s ostatními závodníky</h3>

    <div class="accordion" id="accordionExample">
      <!-- porovnani  -->
      <div class="accordion-item">
        <h2 class="accordion-header" id="headingOne">
          <button
            class="accordion-button"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#collapseOne"
            aria-expanded="true"
            aria-controls="collapseOne"
          >
            Porovnání s vítězem / průměr závodníků
          </button>
        </h2>
        <div
          id="collapseOne"
          class="accordion-collapse collapse show"
          aria-labelledby="headingOne"
        >
          <div class="accordion-body">
            <div class="row g-6">
              <div class="col-lg-6">
                <canvas id="comparisonChart"></canvas>
              </div>
              <div class="col-lg-6">
                <div class="d-flex flex-column gap-4">
                  <div class="rectangleItem">
                    <div class="rectangleItem__stats rectangleItem__stats--win">
                      <div class="text-md">47</div>
                    </div>
                    <p class="text-md">
                      Kolik závodníku <strong>jste předběhl/a</strong>
                    </p>
                  </div>
                  <div class="rectangleItem">
                    <div
                      class="rectangleItem__stats rectangleItem__stats--lose"
                    >
                      <div class="text-md">198</div>
                    </div>
                    <p>
                      Kolik závodníku
                      <strong>vás předběhl/o </strong>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- porovnani end  -->

      <!-- general  -->
      <div class="accordion-item">
        <h2 class="accordion-header" id="headingTwo">
          <button
            class="accordion-button collapsed"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#collapseTwo"
            aria-expanded="false"
            aria-controls="collapseTwo"
          >
            <img src="images/ico/lev.svg" alt="" /> Generali Investments FINISH
          </button>
        </h2>
        <div
          id="collapseTwo"
          class="accordion-collapse collapse"
          aria-labelledby="headingTwo"
        >
          <div class="accordion-body">
            <div
              class="row g-6 justify-content-xl-between justify-content-center"
            >
              <div class="col-xl-4 col-md-8">
                <div class="comparisonGeneral comparisonGeneral--main">
                  <div class="d-flex gap-4 align-items-center">
                    {% include "@images/cas.svg" %}
                    <div class="">
                      <p>Váš čas:</p>
                      <div class="text-large fw-bold">01:37:23:09</div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-xl-4 col-md-8">
                <div class="comparisonGeneral">
                  <div class="tag">Porovnání s vítězem</div>
                  <div class="d-flex gap-3 align-items-center w-100">
                    {% include "@images/stopky-male.svg" %}
                    <div class="">
                      <p>
                        Výsledný čas:
                        <span class="d-block fw-bold">00:00:31</span>
                      </p>
                    </div>
                  </div>
                  <div class="d-flex gap-3 align-items-center w-100">
                    {% include "@images/stopky-male.svg" %}
                    <div class="">
                      <p>
                        Ztráta na nejlepšího závodníka:
                        <span class="d-block fw-bold">00:00:31.19</span>
                      </p>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-xl-4 col-md-8">
                <div class="comparisonGeneral">
                  <div class="tag">Porovnání s průměrným závodníkem</div>
                  <div class="d-flex gap-3 align-items-center w-100">
                    {% include "@images/stopky-male.svg" %}
                    <div class="">
                      <p>
                        Výsledný čas:
                        <span class="d-block fw-bold">00:00:31</span>
                      </p>
                    </div>
                  </div>
                  <div class="d-flex gap-3 align-items-center w-100">
                    {% include "@images/stopky-male.svg" %}
                    <div class="">
                      <p>
                        Ztráta na nejlepšího závodníka:
                        <span class="d-block fw-bold">00:00:31.19</span>
                      </p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- general end  -->

      <!-- Čas na Conti Speed prémii  -->
      <div class="accordion-item">
        <h2 class="accordion-header" id="headingThree">
          <button
            class="accordion-button collapsed"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#collapseThree"
